{
	"name": "my-app",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "yarn smui-theme-light && yarn smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.10",
		"@neoconfetti/svelte": "^1.0.0",
		"@playwright/test": "^1.28.1",
		"@smui/form-field": "^7.0.0-beta.14",
		"@smui/menu": "^7.0.0-beta.14",
		"@smui/switch": "^7.0.0-beta.14",
		"@smui/textfield": "^7.0.0-beta.14",
		"@supabase/supabase-js": "^2.33.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-netlify": "^2.0.8",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.20.4",
		"@types/cookie": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"carbon-components-svelte": "^0.80.0",
		"carbon-icons-svelte": "^12.3.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"svelte-hero-icons": "^5.0.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"type": "module",
	"dependencies": {
		"@smui/button": "^7.0.0-beta.14",
		"@smui/snackbar": "^7.0.0-beta.15",
		"@supabase/auth-helpers-sveltekit": "^0.10.2",
		"autoprefixer": "^10.4.15",
		"flowbite": "^1.8.1",
		"postcss": "^8.4.28",
		"smui-theme": "^7.0.0-beta.14",
		"svelte-markdown": "^0.4.0",
		"tailwindcss": "^3.3.3"
	}
}
